EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=
EMAIL_PORT=
EMAIL_USE_TLS=


# -------------------------------------- #
# Environment variable for cloudinary
# -------------------------------------- #
CLOUD_NAME=
API_KEY=
API_SECRET=


# -------------------------------------- #
# DOMAIN NAME FRONTEND
# -------------------------------------- #
DOMAIN_FRONTEND=localhost:3000
