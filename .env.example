# ---------------------------------------------------- #
# Enviroment variables for the application (SECRET_KEY)
# ---------------------------------------------------- #
SECRET_KEY=


# ---------------------------------------------------- #
# Admin URL (ADMIN_URL)
# ---------------------------------------------------- #
# ADMIN_URL=


# ---------------------------------------------------- #
# (ex: "example.com example2.com ...")
# ---------------------------------------------------- #
ALLOWED_HOSTS="127.0.0.1 localhost"


# --------------------------------- #
# Enviroment variables for database
# --------------------------------- #
# "TYPE_DATABASE" ex: postgresql | sqlite3 | mysql ...
TYPE_DATABASE=sqlite3
# "ENGINE" ex: if TYPE_DATABASE=postgresql then ENGINE=django.db.backends.postgresql
DB_ENGINE=django.db.backends.postgresql
DB_NAME=
DB_USER=
DB_PASSWORD=
DB_HOST=
DB_PORT=


# -------------------------------------- #
# Environment variable for sending emails
# -------------------------------------- #
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.gmail.com
EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=
EMAIL_PORT=587
EMAIL_USE_TLS=True


# -------------------------------------- #
# Environment variable for Redis
# -------------------------------------- #
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=


# -------------------------------------- #
# Environment variable for cloudinary
# -------------------------------------- #
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=


# -------------------------------------- #
# The list of origins allowed to make http requests separated by space (ex: "http://localhost:3000 http://127.0.0.1:3000")
# -------------------------------------- #
CORS_ALLOWED_ORIGINS="http://localhost:3000 http://127.0.0.1:3000"


# -------------------------------------- #
# DOMAIN NAME FRONTEND (ex: localhost:3000)
# -------------------------------------- #
DOMAIN_FRONTEND=http://localhost:3000


# -------------------------------------- #
# Environment variable for Google OAuth
# -------------------------------------- #
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=