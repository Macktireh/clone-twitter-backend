# ---------------------------------------------------- #
# Enviroment variables for the application (SECRET_KEY)
# ---------------------------------------------------- #
SECRET_KEY=
# "DEVELOP_APPS" SECURITY WARNING: do not leave to True because it adds the application of the development dependencies
DEVELOP_APPS=True


# --------------------------------- #
# Enviroment variables for database
# --------------------------------- #
# "TYPE_DATABASE" ex: postgresql | sqlite3 | mysql ...
TYPE_DATABASE=sqlite3
# "ENGINE" ex: if TYPE_DATABASE=postgresql then ENGINE=django.db.backends.postgresql
ENGINE=
DB=
USERNAME=
PASSWORD=
HOST=
PORT=


